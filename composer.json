{
    "name": "aviator/html",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Aviator\\Html\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "require": {
        "php" : ">=7.2.0",
        "cocur/slugify": "^3.0",
        "aviator/delegate": "1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "7.1.x",
        "nesbot/carbon": "^1.22",
        "dbt/phpunit-runner": "^0.1.2",
        "vimeo/psalm": "^3.2",
        "larapack/dd": "^1.1"
    },
    "autoload-dev": {
        "psr-4": {
            "Aviator\\Html\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "php ./vendor/dbt/phpunit-runner/phpunit.php --no-coverage",
        "coverage": "phpunit --coverage-html coverage",
        "stat": "psalm"
    }
}
